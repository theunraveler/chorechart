<% content_for :page_title, "Edit #{@group}" %>

<div class="page-header">            
  <h1><%= t("web-app-theme.edit", :default => "Edit") %> <%= @group %></h1>
</div>

<%= form_for @group, :url => group_path(@group), :html => { :class => :form } do |f| -%>
  <%= render :partial => "form", :locals => {:f => f} %>
<% end -%>
